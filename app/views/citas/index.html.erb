<h1>Listing citas</h1>

<table>
  <tr>
    <th>Id</th>
    <th>User</th>
    <th>Asunto</th>
    <th>Fecha</th>
    <th>Hora</th>
  </tr>

<% @citas.each do |cita| %>
  <tr>
    <td><%=h cita.id %></td>
    <td><%=h cita.user_id %></td>
    <td><%=h cita.asunto %></td>
    <td><%=h cita.fecha %></td>
    <td><%=h cita.hora %></td>
    <td><%= link_to 'Show', cita %></td>
    <td><%= link_to 'Edit', edit_cita_path(cita) %></td>
    <td><%= link_to 'Destroy', cita, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New cita', new_cita_path %>